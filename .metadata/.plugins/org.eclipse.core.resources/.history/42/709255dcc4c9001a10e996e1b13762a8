import java.io.*;
import java.util.*;

public class Binary_Search {
	public static void main(String[] args) throws IOException{
		
		int[] nums = {3,3,3};
		
		System.out.println(binary_search_repeating_nums(nums, 3));
		
	}
	static int binary_search(int[] nums, int val){
		int start = 0;
		int end = nums.length - 1;
		while (start <= end) // ???
		{
			int middle = (start + end) / 2; // ???
			if (nums[middle] == val)
			{
				return middle;
			}
			else if (nums[middle] > val)
			{
				end = middle - 1; // ???
			}
			else
			{
				start = middle + 1; // ???
			}
		}
		return -1;
	}
	static int binary_search_repeating_nums(int[] nums, int val){
		int start = 0;
		int end = nums.length - 1;
		while (start < end) // ???
		{
			int middle = (start + end) / 2; // ???
			if (nums[middle] >= val)
			{
				end = middle; // ???
			}
			else
			{
				start = middle + 1; // ???
			}
		}
		if (nums[start] == val)
		{
			return start;
		}
		else
		{
			return -1;
		}
	}
}
